{
  "network": {
    "servers": [ "logstash:5043" ],
    "ssl certificate": "/opt/certs/logstash-forwarder.crt",
    "ssl key": "/opt/certs/logstash-forwarder.key",
    "ssl ca": "/opt/certs/logstash-forwarder.crt",
    "timeout": 15
  },
  "files": [
    {
      "paths": [ "/tmp/test/test.log" ],
      "fields": { "type": "stdin" }
    }
  ]
}
